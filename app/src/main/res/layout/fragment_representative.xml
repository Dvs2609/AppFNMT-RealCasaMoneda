<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollview"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.RepresentativeCertificates.Representative">

    <RelativeLayout
        android:id="@+id/gridGetCertificates"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="18dp">

        <TextView
            android:id="@+id/oc_repre_init_txt"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:text="@string/oc_repre_title"
            android:textColor="@color/black"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/oc_repre_back_cross"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="5dp"
            android:src="@drawable/cross_svg"
            app:tint="@color/icon_arrow_svg" />

        <TextView
            android:id="@+id/oc_repre_nav"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/oc_repre_init_txt"
            android:text="@string/txt_oc_repre_nav"
            android:textSize="12sp" />

        <RelativeLayout
            android:id="@+id/RL_grid_oc_repre"
            android:layout_width="match_parent"
            android:layout_below="@id/oc_repre_nav"
            android:layout_height="wrap_content"
            android:layout_marginTop="35dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:text="@string/questionCertificates_ObtainCertificates_fr"
                android:textColor="@color/black"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/iconHelp"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentTop="true"
                android:layout_toStartOf="@+id/txtHelp"
                android:layout_marginTop="4dp"
                android:src="@drawable/icon_question_svg"
                app:tint="@color/icons_svg" />

            <TextView
                android:id="@+id/txtHelp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:text="@string/helptxt_ObtainCertificates_fr"
                android:textColor="@color/icons_svg"
                android:textSize="14sp" />

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtHelp"
                android:layout_marginTop="25dp"
                android:columnCount="2"
                android:rowCount="3">
                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="0"
                    android:layout_rowWeight="0"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginBottom="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="33dp"
                                android:layout_height="33dp"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:src="@drawable/icon_suitcase_svg"
                                app:tint="@color/icons_svg" />

                            <ImageView
                                android:id="@+id/icon_GoTo_singleAdmin"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="80dp">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="@string/oc_repre_grid_Uniq_Admin_title"
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="0"
                    android:layout_rowWeight="0"
                    android:layout_column="1"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginStart="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="33dp"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:src="@drawable/icon_employe_src"
                                app:tint="@color/icons_svg" />

                            <ImageView
                                android:id="@+id/icon_GoTo_LegalEntity"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="80dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="@string/oc_repre_grid_Jurid_Person_title"
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="1"
                    android:layout_rowWeight="0"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginBottom="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="30dp"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:src="@drawable/icon_institution_svg"
                                app:tint="@color/icons_svg" />

                            <ImageView
                                android:id="@+id/icon_GoTo_NonLegalEntity"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="100dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="@string/oc_repre_grid_NoJurid_Entity_title"
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="1"
                    android:layout_rowWeight="0"
                    android:layout_column="1"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginBottom="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="28dp"
                                android:layout_height="30dp"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:src="@drawable/icon_suitcase_svg"
                                app:tint="@color/icons_svg" />

                            <ImageView
                                android:id="@+id/icon_GoTo_OtherOptions"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="100dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="@string/oc_repre_grid_Options_title"
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>



            </GridLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="35dp"
            android:orientation="vertical"
            android:layout_below="@+id/RL_grid_oc_repre">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/oc_repre_title1"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/oc_repre_text"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="15dp"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:text="@string/oc_repre_text"/>

            <RelativeLayout
                android:id="@+id/step3_goTo1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="30dp"
                android:layout_below="@+id/oc_repre_text"
                android:background="@drawable/shape_corners_relative_layout">

                <TextView
                    android:layout_width="280dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:textSize="14sp"
                    android:textColor="@color/icons_svg"
                    android:text="@string/oc_repre_go_to_help"
                    android:layout_marginStart="15dp"/>

                <ImageView
                    android:id="@+id/icon_GoTo_help"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="19dp"
                    android:layout_marginEnd="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:src="@drawable/right_arrow_svg"
                    app:tint="@color/icon_arrow_svg" />
            </RelativeLayout>

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="30dp"
                android:layout_below="@+id/oc_Step1_textDesc"
                android:columnCount="1"
                android:rowCount="3">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardtxt_repre_UniqueAdmin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="0"
                    android:layout_rowWeight="0"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginBottom="10dp"

                    android:layout_marginTop="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="270dp"
                                android:layout_height="match_parent"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:textColor="@color/icons_svg"
                                android:text="@string/oc_repre_grid_Uniq_Admin_title"
                                app:tint="@color/icons_svg"/>

                            <ImageView
                                android:id="@+id/repre_icon_dropdown1"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/RLReminderDropdown"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="gone">
                            <TextView
                                android:id="@+id/oc_repre_text_Dropdown"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="15dp"
                                android:textColor="@color/black"
                                android:textSize="14sp"
                                android:text="@string/oc_repre_dropdown1_text" />

                            <RelativeLayout
                                android:id="@+id/rectangularLYPhysicalPerson"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/oc_repre_text_Dropdown"
                                android:background="@drawable/rectangular_shape"
                                android:layout_marginTop="15dp">

                                <TextView
                                    android:layout_width="270dp"
                                    android:layout_height="45dp"
                                    android:backgroundTint="#EFEFF0"
                                    android:textColor="@color/icons_svg"
                                    android:layout_marginStart="13dp"
                                    android:textAlignment="viewStart"
                                    android:textAllCaps="false"
                                    android:layout_marginTop="0dp"
                                    android:textSize="14sp"
                                    android:text="@string/oc_repre_dropdown1_go_to_request_unq_admin"
                                    tools:ignore="RtlCompat" />

                                <ImageView
                                    android:id="@+id/iconRepreGoToReqUniq"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentEnd="true"
                                    android:src="@drawable/right_arrow_svg"
                                    app:tint="@color/icon_arrow_svg" />

                            </RelativeLayout>
                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardtxt_repre_juridic_person"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="1"
                    android:layout_rowWeight="0"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:textColor="@color/icons_svg"
                                android:text="@string/oc_repre_grid_Jurid_Person_title"
                                app:tint="@color/icons_svg"/>

                            <ImageView
                                android:id="@+id/repre_icon_dropdown2"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/RLJuridicDropdown"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="gone">
                            <TextView
                                android:id="@+id/repre_text_Dropdow2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="15dp"
                                android:textColor="@color/black"
                                android:textSize="14sp"
                                android:text="@string/oc_repre_dropdown2_text"/>

                            <RelativeLayout
                                android:id="@+id/rectangularLYJuridic"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/repre_text_Dropdow2"
                                android:background="@drawable/rectangular_shape"
                                android:layout_marginTop="15dp">

                                <TextView
                                    android:layout_width="270dp"
                                    android:layout_height="45dp"
                                    android:backgroundTint="#EFEFF0"
                                    android:textColor="@color/icons_svg"
                                    android:layout_marginStart="13dp"
                                    android:textAlignment="viewStart"
                                    android:textAllCaps="false"
                                    android:layout_marginTop="0dp"
                                    android:textSize="14sp"
                                    android:text="@string/oc_repre_dropdown2_go_to_request_juridic"
                                    tools:ignore="RtlCompat" />

                                <ImageView
                                    android:id="@+id/iconRepreGoToJuridic"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentEnd="true"
                                    android:src="@drawable/right_arrow_svg"
                                    app:tint="@color/icon_arrow_svg" />

                            </RelativeLayout>

                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardtxt_repre_juridic_entity"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_row="2"
                    android:layout_rowWeight="0"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    app:cardBackgroundColor="#EFEFF0"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="270dp"
                                android:layout_height="match_parent"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:textColor="@color/icons_svg"
                                android:text="@string/oc_repre_grid_NoJurid_Entity_title"
                                app:tint="@color/icons_svg"/>

                            <ImageView
                                android:id="@+id/repre_icon_dropdown3"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_marginTop="15dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/right_arrow_svg"
                                app:tint="@color/icon_arrow_svg" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/RLSoftwareNecesaryDropdown"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="gone">
                            <TextView
                                android:id="@+id/repre_text_Dropdown3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="14dp"
                                android:textColor="@color/black"
                                android:textSize="14sp"
                                android:text="@string/oc_repre_dropdown3_text"/>


                            <RelativeLayout
                                android:id="@+id/rectangularLY"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/repre_text_Dropdown3"
                                android:background="@drawable/rectangular_shape"
                                android:layout_marginTop="15dp">

                                <TextView
                                    android:layout_width="290dp"
                                    android:layout_height="wrap_content"
                                    android:backgroundTint="#EFEFF0"
                                    android:textColor="@color/icons_svg"
                                    android:layout_marginStart="10dp"
                                    android:textAlignment="viewStart"
                                    android:textAllCaps="false"
                                    android:layout_marginTop="2dp"
                                    android:layout_marginBottom="3dp"
                                    android:textSize="14sp"
                                    android:text="@string/oc_repre_dropdown3_go_to_request_nojuridic"
                                    tools:ignore="RtlCompat" />


                                <ImageView
                                    android:id="@+id/iconStep1GoToDownloadArea"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_marginTop="12dp"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentEnd="true"
                                    android:src="@drawable/right_arrow_svg"
                                    app:tint="@color/icon_arrow_svg" />

                            </RelativeLayout>
                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

            </GridLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/oc_repre_title2"
                android:textStyle="bold"
                android:layout_marginTop="0dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawablePadding="10dp"
                app:drawableLeftCompat="@drawable/dot_svg"
                android:layout_marginTop="20dp"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:text="@string/oc_repre_dropdown3_title2_list1"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawablePadding="10dp"
                app:drawableLeftCompat="@drawable/dot_svg"
                android:layout_marginTop="20dp"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:text="@string/oc_repre_dropdown3_title2_list2"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawablePadding="10dp"
                app:drawableLeftCompat="@drawable/dot_svg"
                android:layout_marginTop="20dp"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:text="@string/oc_repre_dropdown3_title2_list3"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawablePadding="10dp"
                app:drawableLeftCompat="@drawable/dot_svg"
                android:layout_marginTop="20dp"
